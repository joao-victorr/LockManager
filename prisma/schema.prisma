// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id @default(cuid())
  name String
  email String @unique
  password String

  @@map("users")
}


model Lock {
  id String @id @default(cuid())
  name String @unique
  ip String @unique

  @@map("lock")
}

model Access {
  id String @id @default(cuid())
  name String
  image Bytes
  

  @@map("access")
}